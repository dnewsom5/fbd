					lang: 'en-US',
					otUseStrict: false,
					disableCookies: 0,
					isGovtSite: false,
					ipmEnabled: false,
					ipmOverlaysPermitted: false,
					concurAccessible: false,
					prodStatus: 'qa',
					localizationMode: false,
					isGateway: true,
					brandingId: 11,
					category: 'Expense',
					lockTime: 3600,
					logoutTime: 3600,
					analyticsId: 'E107B0D2AE14A14CA1C1F19BF9EC56189EBF9E112E7E6F1528CEDB83C3F310A2',
					gatewayPreviewDaysRemaining: 0,
					companyName: 'RandomVerbs.com',
					environment: 'DotNet'
				});
				$.extend( cnqr.namespace( 'config.number' ), {
					negativeFormat: '-',
					thouSep: ',',
					decSep: '.'
				});
				$.extend( cnqr.namespace( 'config.currency' ), {
					negativeFormat: '-',
					symbolLocation: 'before'
				});
				$.extend( cnqr.namespace( 'config.date' ), {
					format : {
						closeText: 'Done',
						prevText: 'Prev',
						nextText: 'Next',
						currentText: 'Today',
						monthNames: ['January','February','March','April','May','June','July','August','September','October','November','December',''],
						monthNamesShort: ['Jan','Feb','Mar','Apr','May','Jun','Jul','Aug','Sep','Oct','Nov','Dec',''],
						dayNames: ['Sunday','Monday','Tuesday','Wednesday','Thursday','Friday','Saturday'],
						dayNamesShort: ['Sun','Mon','Tue','Wed','Thu','Fri','Sat'],
						dayNamesMin: ['Su','Mo','Tu','We','Th','Fr','Sa'],
						weekHeader: 'Wk',
						dateFormat: 'mm/dd/yy',
						firstDay: 0,
						isRTL: false,
						showMonthAfterYear: false,
						yearSuffix: ''
					}
				});
				$.extend( cnqr.namespace( 'config.time' ), {
					militaryTime: false,
					separator: ':',
					format: 'h:mm tt'
				});

				$.extend( $.cnqr.config, {
					isGateway: true,
					isOldIE: false,
					IPMForOldIEDisabled: false
				});
			})( $.cnqr );
		</script>

